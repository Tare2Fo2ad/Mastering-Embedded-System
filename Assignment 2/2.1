#include <stdio.h>

int main()
{


	int num;
	printf ("Enter a number:");
	fflush(stdout); fflush(stdin);
	scanf("%d",&num);
	fflush(stdout); fflush(stdin);
	if (num % 2==0)
	{
		printf("The number %d is even ",num);
	}
	else
	{
		printf("The number %d is odd ",num);
	}
	return 0;

}
