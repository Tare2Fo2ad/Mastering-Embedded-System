#include <stdio.h>

int main ()

{

	float a;

	printf("Enter a Number = ");
	fflush(stdout);
	fflush(stdin);
	scanf("%f",&a);

	if (a>0)
	{
		printf("%f is Positive ",a);
	}
	else if (a<0)

	{
		printf("%f is Negative ",a);

	}
	else
	{
		printf("You entered a ZERO");
	}
	return 0;

}



